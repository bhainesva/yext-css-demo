{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist"
      }
    ],
    "soyTemplates": [
      {
        "root": "templates",
        "pattern": "**/*.soy"
      }
    ]
  },
  "base_image_tag": "yextci/yextci-builder:stable",
  "dependencies": {
    "installDepsCmd": "yarn",
    "requiredFiles": [
      "package.json"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "yarn build"
  }
}