{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist"
      }
    ],
    "soyTemplates": [
      {
        "root": "templates",
        "pattern": "**/*.soy"
      }
    ]
  },
  "base_image_tag": "yextci/yextci-builder:stable",
  "dependencies": {
    "installDepsCmd": "echo install;",
    "requiredFiles": [
      "package.json"
    ]
  },
  "buildArtifacts": {
    "build_setup_cmd": "npm install",
    "build_cmd": "npm run build"
  },
  "livePreview": {
    "serve_setup_cmd": "echo setup",
    "serve_cmd": "echo serve"
  },
  "watch_cmd": "npm run watch"
}